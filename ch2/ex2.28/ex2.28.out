1 ]=> ; fringe proc
(define (fringe xs)
  (define (iter xs acc)
    (cond ((null? xs) acc)
          ((not (pair? xs)) (cons xs acc))
          (else (iter (car xs)
                      (iter (cdr xs) acc)))))
  (iter xs nil))
;Value: fringe

