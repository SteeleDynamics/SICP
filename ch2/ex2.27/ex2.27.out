1 ]=> ; reverse procedure
(define (reverse xs)
  (define (iter xs acc)
    (if (null? xs)
        acc
        (iter (cdr xs) (cons (car xs) acc))))
  (iter xs nil))
;Value: reverse

1 ]=> ; deep-reverse procedure
(define (deep-reverse xs)
  (define (iter xs acc)
    (cond ((null? xs) acc)
          ((not (pair? xs)) xs)
          (else (iter (cdr xs)
                      (cons (iter (car xs) nil)
                            acc)))))
  (iter xs nil))
;Value: deep-reverse

1 ]=> ; unit tests for reverse and deep-reverse
(deep-reverse '((1 2) (3 4)))
;Value: ((4 3) (2 1))

