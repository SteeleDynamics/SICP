; AXIOM FOR PAIRS
;
; For any x and y
; 
; (car (cons x y)) is x
; (cdr (cons x y)) is y
;

(define (cons a b)
  (lambda (pick)
    (cond ((= pick 1) a)
          ((= pick 2) b))))
;Value: cons

