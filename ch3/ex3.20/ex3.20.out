1 ]=> ; cdr selector procedure
(define (cdr z) (z 'cdr))
;Value: cdr

1 ]=> ; set-car! mutator procedure
(define (set-car! z new-value)
  ((z 'set-car!) new-value)
  z)
;Value: set-car!

1 ]=> ; set-cdr! mutator procedure
(define (set-cdr! z new-value)
  ((z 'set-cdr!) new-value)
  z)
;Value: set-cdr!

